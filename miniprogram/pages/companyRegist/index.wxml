<!--pages/companyRegist/index.wxml-->
<div class="regist_tab">
 <div class="title">
        <p>注册账户</p>
  </div>
  <ul>
    <li>
      <div class="item">
        <p>公司名称:</p>
      </div>
      <div class="input_tab">
        <input type="text" name="name" placeholder="请输入公司名称" placeholder-class="placeholderStyle"></input>
      </div>
    </li>
     <li>
      <div class="item">
        <p>联系人:</p>
      </div>
      <div class="input_tab">
        <input type="text" name="username" placeholder="请输入联系人" placeholder-class="placeholderStyle"></input>
      </div>
    </li>
     <li>
      <div class="item">
        <p>手机号码:</p>
      </div>
      <div class="input_tab">
        <input type="text" name="phone" placeholder="请输入手机号码" placeholder-class="placeholderStyle"></input>
      </div>
    </li>
     <li>
      <div class="item">
        <p>验证码:</p>
      </div>
      <div class="input_tab">
        <input type="text" name="checkcode" placeholder="请输入验证码" placeholder-class="placeholderStyle"></input>
        <div class="getcode">
          <button>获取验证码</button>
        </div>
      </div>
    </li>
     <li>
      <div class="item">
        <p>图片验证:</p>
      </div>
      <div class="input_tab">
        <input type="text" name="imgcode" placeholder="请输入验证码" placeholder-class="placeholderStyle"></input>
        <div class="getcode">
          <button>获取验证码</button>
        </div>
      </div>
    </li>
     <li>
      <div class="item">
        <p>设置密码:</p>
      </div>
      <div class="input_tab">
        <input type="password" name="password" placeholder="请输入密码" placeholder-class="placeholderStyle"></input>
      </div>
    </li>
     <li>
      <div class="item">
        <p>确认密码:</p>
      </div>
      <div class="input_tab">
        <input type="repassword" name="repassword" placeholder="请输入密码" placeholder-class="placeholderStyle"></input>
      </div>
    </li>
     <li>
     <checkbox class="agree"></checkbox>
      <p>注册即同意<a>《常州市人才引育服务协议》</a></p>
    </li>
    <li>
      <div class="submit">
        <button>登录</button>
      </div>
    </li>
  </ul>
</div>


