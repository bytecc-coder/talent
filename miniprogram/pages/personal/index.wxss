/**index.wxss**/
.top_Img{
  width: 100%;
  display: block;
}
.top_Img image{
  display: block;
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin: 100rpx auto;
}

.infos ul{
  width: 88%;
  margin: 20rpx auto;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.infos li{
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
}
.infos_left{
  display: flex;
  width: 60%;
  height: 50rpx;
}
.infos_left p{
  display: block;
  font-size: 26rpx;
  justify-content: flex-start;
}
.infos_right{
  display: flex;
  width: 40%;
  height: 50rpx;
  justify-content: flex-end;
}
.infos_right image{
  display: block;
  width: 40rpx;
  height: 40rpx;
}
.bottom_info{
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.bottom_info p{
  display: block;
  font-size: 20rpx;
  text-align: center;
}