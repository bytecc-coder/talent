page{
  background-color: #f4f4f4;
}

image{
  width: 750rpx;
  height: 300rpx;
  display: inline-block;
  overflow: hidden;
}
.four-img{
  width: 80rpx;
  height: 80rpx;
  margin: auto;
  background:url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 50rpx 50rpx;
  background-position-y: center;
  background-position-x: center;
}
.row-model{
  display: block;
  padding:30rpx 40rpx;
  text-align: center;
  font-size: 20rpx;
}
.four-img image{
  width: 80rpx;
  height: 80rpx;
}
van-panel{
  display: block;
  text-align: center;
  /* border: 1rpx solid #f4f4f4; */
  box-shadow: unset;
}
.van-cell{
  background: #f4f4f4 !important;
  margin-top: 20rpx;
  border: none;
}
.van-cell__title{
  font-size: 40rpx;
  border: none;
}
.van-cell__label{
  line-height: 56rpx !important;
}
.promise{
  padding: 0 48rpx;
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 20rpx;
}
.promise-left{
    padding: 30rpx;
    background: #e9e9e9;
    width: 44%;
    height: 230rpx;
    border-bottom-left-radius: 20rpx;
    border-top-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
    border-top-right-radius: 20rpx;
    text-align: center;
}
.promise-left>span{
  display: block;
  font-size: 22rpx;
}
.promise-left>p{
  display: block;
  color: #bebebe;
  font-size: 16rpx;
  margin-top: 8rpx; 
  margin-bottom: 30rpx;
}
.promise-left>div{
  display: block;
  width: 50%;
  height: 60%;
  overflow: hidden;
  margin: auto;
}
.promise-left>div>image{
  width: 100%;
  height: 100%;
}
.promise-right{
  display: flex;
  height: 290rpx;
  width: 276rpx;
  flex-direction: column;
  justify-content: space-between;
  overflow: hidden;
}
.promise-right>div{
  width: 78%;
  height: 26%;
  background: #e9e9e9;
  border-top-right-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  border-top-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  padding:30rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  overflow: hidden;
}
.promise-right>div>div{
  display: block;
}

.promise-right>div>div>span{
  display: block;
  font-size: 22rpx;
}
.promise-right>div>div>p{
   display: block;
  color: #bebebe;
  font-size: 16rpx;
  margin-top: 8rpx; 
  margin-bottom: 10rpx;
}
.promise-right>div>image{
  display: block;
  width: 70rpx;
  height: 70rpx;
}

p{
  font-size: 30rpx;
  text-align: center;
  letter-spacing: 1rpx;
}
.jobs{
  width: 100%;
  height: auto;
}
.jobs ul{
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.jobs li{
  width: 92%;
  height: 200rpx;
  position: relative;
  right: 0;
  left: 0;
  margin:20rpx auto;
  padding: 0 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  background: #fff;
  text-align: center;
}
.first{
  width: 100%;
  height: 60rpx;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: space-between;

}
.stander{
  width: 80%;
  height: 50rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}
.third{
  width: 100%;
  height: 90rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}
.tags{
  width: 60%;
  height: auto;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.btns{
  width: 35%;
  height: 60rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-end;
}
.van-button--info{
  background: #36a8fe!important;
  border-top-right-radius: 6rpx;
  border-bottom-left-radius: 6rpx;
  border-top-left-radius: 6rpx;
  border-bottom-right-radius: 6rpx;
  border: 0!important;
}
.salary{
  font-size: 28rpx;
  color: #36a8fe;
  
}
.job_title{
  font-size: 30rpx;
  color: rgba(0, 0, 0, 0.7);
}
.stander p{
  font-size: 26rpx;
  color:  rgba(0, 0, 0, 0.5);
}
.tags p{
  display: block;
  height: 30rpx;
  line-height: 26rpx;
  padding: 10rpx;
  background: #EDEDED;
  font-size: 26rpx;
  border-radius: 10rpx;
  color: rgba(0, 0, 0, 0.3)
}
